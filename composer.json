{
  "name": "omar-haris/arabic-name-transliterator",
  "description": "A PHP library to transliterate Arabic names to English names can handle Arabic names with diacritics and special characters.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Omar Haris",
      "email": "omar@haris.bz"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "ArabicNameTransliterator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArabicNameTransliterator\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "scripts": {
    "test": "phpunit"
  },
  "homepage": "https://github.com/omar-haris/arabic-name-transliterator",
  "keywords": [
    "arabic",
    "transliteration",
    "transliterate",
    "transliterator",
    "transliterator",
    "arabic-transliterator",
    "arabic-name-transliterator",
    "iraqi-transliterator",
    "iraqi-name-transliterator",
    "arabian-transliterator",
    "arabian-name-transliterator",
    "arabic-transliterate",
    "iraqi-transliterate",
    "arabian-transliterate",
    "english-name",
    "english-names",
    "english-name-transliterator",
    "english-names-transliterator",
    "english-name-transliterator",
    "english-names-transliterator",
    "arabic-to-english",
    "iraqi-to-english",
    "arabian-to-english",
    "english-to-arabic",
    "english-to-iraqi",
    "english-to-arabian"
  ],
  "support": {
    "issues": "https://github.com/omar-haris/arabic-name-transliterator/issues",
    "source": "https://github.com/omar-haris/arabic-name-transliterator"
  }
}
